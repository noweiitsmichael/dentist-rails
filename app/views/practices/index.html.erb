          <div class="col-md-2 col-sm-6">
              <div class="widget widget-stats bg-black recommendations-day" day="<%= i %>">
                  <div class="stats-icon stats-icon-lg"><i class="fa fa-globe fa-fw"></i></div>
                  <div class="stats-title"><%= (Time.now + i.days).strftime("%B %e") %></div>
                  <% if i == 0 %>
                    <div class="stats-number">TODAY</div>
                  <% else %>
                    <div class="stats-number"><%= (Time.now + i.days).strftime("%A") %></div>
                  <% end %>
              </div>

              <% if i == 0 %>
                <div style="position:relative"><div style="display:block" class="arrow-down"></div></div>
              <% else %>
                <div style="position:relative"><div class="arrow-down"></div></div>
              <% end %>
          </div>
        <% end %>
      </div>
